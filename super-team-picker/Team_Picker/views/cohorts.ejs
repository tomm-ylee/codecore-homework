<%- include('./partials/header') %>
    <h1>Cohorts</h1>
    <% for(let cohort of allCohorts) { %>
      <div class="container card" style="width: 18rem">
        <% if(cohort.logo_url) { %>
          <img class="card-img-top" src="<%= cohort.logo_url %>"/></p>
        <% } %>

        <h5><%= cohort.cohort_name %></h5>
        <p><%= cohort.members %></p>
        <a href="/cohorts/<%=cohort.id%>" class="btn btn-primary">Show Cohort</a>
      </div>
    <% } %>

<%- include('./partials/footer') %>
