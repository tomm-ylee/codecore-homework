<%= form_for :session, url: session_path do |f| %>
  <div>
    <%= f.label :email %><br>
    <%= f.email_field :email, class: "form-control"%>
  </div>

  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password, class: "form-control"%>
  </div>

  <%= f.submit 'Sign In', class: "btn btn-outline-primary"%>
    <%= link_to 'Cancel', root_path %>
<% end %>
