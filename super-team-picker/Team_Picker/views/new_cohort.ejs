<%- include('./partials/header') %>
    <h1>New Cohort</h1>

    <form method="post">
      <div>
        <label for="logoUrl">Logo Url</label>
        <br/>
        <input class="input-text" type="text" name="logoUrl">
        <br/>
      </div>
      <div>
        <label for="cohortName">Name</label>
        <br/>
        <input class="input-text" type="text" name="cohortName">
        <br/>
      </div>
      <div>
        <label for="members">Members</label>
         <br/>
        <textarea class="input-text" name="members" rows="5" cols="50"></textarea>
      </div>

      <input class="btn btn-primary" type="submit" value="Submit">

    </form>
<%- include('./partials/footer') %>
